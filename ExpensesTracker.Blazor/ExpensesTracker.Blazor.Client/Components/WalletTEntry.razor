<tr style="text-align: center" id="=@Data.EntryId">
    <th scope="row">@Data.Date</th>
    <td>
        <div>
           @Data.Amount
        </div>
    </td>
    <td>
        <div class="table-badge" style="border-color: @Data.Category.ColorCode; background: @Data.Category.ColorCode">
            @Data.Category.Name
        </div>

    </td>
    <td>
        <div class="table-badge" style="border-color: @Data.Label.ColorCode; background: @Data.Label.ColorCode">
            @Data.Label.Name
        </div>
    </td>
    <td style="text-align: end">
        <button class="btn btn-primary">Edit</button>
        <button class="btn btn-danger">Delete</button>
    </td>
</tr>

